						</FontString>
					</Layer>
				</Layers>
                <Frames>
				<Button name="Frameron" parentKey="Frameron" inherits="GMGenie_Window">
					<Size>
						<AbsDimension x="295" y="267"/>
					</Size>
					<Anchors>
						<Anchor point="LEFT" relativeTo="GMGenie_Spy_Misiones_Main" />
					</Anchors>
					<Scripts>
						<OnShow> 
							self:SetPoint("LEFT", "GMGenie_Spy_Misiones_Main", "RIGHT");
						</OnShow>
						<OnLoad> 
							self:ClearAllPoints();
							GMGenie.loadWindow(self, 'Misiones', false, nil);
						</OnLoad>
						<OnDoubleClick>
							self:ClearAllPoints();
							self:SetPoint("LEFT", "GMGenie_Spy_Misiones_Main", "RIGHT");
						</OnDoubleClick>
					</Scripts>
					<Frames>
						<ScrollFrame name="$parent_Croll" inherits="frame" parentKey="FrameScroll">
